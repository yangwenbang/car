<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.car.dao.ProvinceDao">

	<select id="getProvinces" resultType="com.car.vo.ProvinceVO">
		SELECT p.id provinceId, p.province_name, p.company_address, p.contact_way 
		FROM province p
	</select>
	
</mapper>