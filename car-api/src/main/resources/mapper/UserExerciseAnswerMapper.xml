<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.car.dao.UserExerciseAnswerDao">

	<insert id="saveUserExerciseAnswer" parameterType="com.car.entity.UserExerciseAnswer">
		INSERT INTO user_exercise_answer
		(user_id,exercise_content_id,exercise_answer,create_time,update_time)
		VALUES (#{userId},#{exerciseContentId},#{exerciseAnswer},#{createTime},#{updateTime})
	</insert>
	
	

</mapper>